#!/bin/bash

ssid=$($SCRIPT_DIR/ssid/ssid)
if [[ -n $ssid ]]; then
    echo $LABEL $ssid
    echo $LABEL $ssid
    echo \#00FF00
else
    iface=$($SCRIPT_DIR/iface/iface | head -n 1)
    if [[ -n $iface ]]; then
        echo $iface
        echo $iface
        echo \#00FF00
    else
		echo $LABEL down
		echo $LABEL down
        echo \#FF0000
    fi
fi
